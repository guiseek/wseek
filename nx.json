    },
    "common-ui-kit": {
      "tags": ["type:ui"]
    },
    "common-util-mock": {
      "tags": []
    },
    "common-chat-panel": {
      "tags": []
    },
    "admin": {
      "tags": ["type:app"]
    },
    "admin-e2e": {
      "tags": [],
      "implicitDependencies": ["admin"]
    },
    "ng-crud": {
      "tags": ["type:plugin"]
    },
    "ng-crud-e2e": {
      "tags": [],
      "implicitDependencies": ["ng-crud"]
    },
    "server": {
      "tags": ["type:app"]
    },
    "global-util-mapper": {
      "tags": ["type:util", "scope:global"]
    }
  }
}
